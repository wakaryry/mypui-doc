<% var isHomePage = page.path === 'index.html' %>

<div class="sidebar">
  <div class="sidebar-inner">
    <% if (isHomePage) { %>
      <ul class="main-menu">
        <%- partial('partials/main_menu', { context: 'sidebar' }) %>
      </ul>
    <% } %>
    <div class="list">
      <% if (type !== 'search') { %>
        <h2>
          <% titles = {
            api: 'API',
            examples: '示例',
            guide: '教程',
			cookbook: '分享',
			tool: '工具'
          } %>
          <%- titles[type] %>
        </h2>
        <%- partial('partials/toc', { type: type }) %>
      <% } %>
    </div>
  </div>
</div>
